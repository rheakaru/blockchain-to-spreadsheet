{
"manifest_version": 2,
    "name": "Replace Rhea with aehr",
    "description": "Replaces all instances of the word \"rhea\" with the phrase \"aehr\".",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
